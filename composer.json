{
  "name": "miaoxing/verify-code",
  "type": "miaoxing-plugin",
  "keywords": [
    "miaoxing",
    "verify-code"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/sms": "^0.1.10",
    "miaoxing/app": "^0.2.10"
  },
  "require-dev": {
    "miaoxing/coding-standards": "^2.0.0"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\VerifyCode\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\VerifyCode\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-types": [
      "miaoxing-plugin"
    ],
    "installer-paths": {
      "plugins/{$name}/": [
        "type:miaoxing-plugin"
      ]
    }
  }
}
